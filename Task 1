
#include <iostream>
using namespace std;

int main()
{
    int A, B;
    cout << "Input A: ";
    cin >> A;
    cout << "Input B: ";
    cin >> B;

    while (A <= B)
    {
        if (A % 2 != 0)
        {
            A += 1;
        }

        if (A != 0) cout << " " << A;

        if (A % 2 == 0)
        {
            A += 2;
        }
    }
    cout << endl;
    system("pause");
}
